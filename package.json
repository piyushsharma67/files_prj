{
  "name": "nufactor-server",
  "version": "1.0.0",
  "description": "Server side api for nufactor form-builder",
  "main": "app.js", 
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "db:create": "sequelize db:create",
    "db:migration": "sequelize migration:create --name",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all"
  },
  "author": "Vimal Kurien Sam",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "sequelize": "^5.22.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "sequelize-cli": "^6.2.0"
  }
}
